{
  "name": "elife/journal-cms",
  "type": "project",
  "authors": [
    {
      "name": "",
      "role": ""
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8",
      "exclude": [
        "drupal/monolog"
      ]
    },
    {
      "type": "vcs",
      "url": "https://github.com/drupal-composer/drupal-project"
    },
    {
      "type": "package",
      "package": {
        "name": "drupal/monolog",
        "type": "drupal-module",
        "version": "dev-custom",
        "source": {
          "type": "git",
          "url": "https://git.drupalcode.org/project/monolog.git",
          "reference": "3109707-drupal-9-readiness"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "zenorocha/clipboardjs",
        "type": "drupal-library",
        "version": "1.7.1",
        "source": {
          "url": "https://github.com/zenorocha/clipboard.js",
          "type": "git",
          "reference": "origin/master"
        }
      }
    },
    {
      "type": "package",
      "package": {
          "name": "codeseven/toastr",
          "version": "2.1.4",
          "type": "drupal-library",
          "dist": {
              "url": "https://github.com/CodeSeven/toastr/archive/refs/tags/2.1.4.zip",
              "type": "zip"
          }
      }
    }
  ],
  "require": {
    "php": "^7.4",
    "aws/aws-sdk-php": "~3.0",
    "codeseven/toastr": "2.1.4",
    "composer/installers": "^1.7",
    "cweagans/composer-patches": "^1.7",
    "drupal/address": "^1@RC",
    "drupal/admin_toolbar": "^3.3",
    "drupal/allowed_formats": "^1.3",
    "drupal/confi": "^3.0",
    "drupal/config_devel": "^1.8",
    "drupal/console": "^1.9",
    "drupal/core-composer-scaffold": "^9.5",
    "drupal/core-recommended": "^9.5",
    "drupal/country": "^1.0",
    "drupal/ctools": "^3.13",
    "drupal/devel": "^5.1",
    "drupal/diff": "^1.0@RC",
    "drupal/duration_field": "^2.1",
    "drupal/entity_reference_revisions": "1.x-dev",
    "drupal/entity_reference_unpublished": "^1.0@alpha",
    "drupal/entityqueue": "^1.5",
    "drupal/field_group": "^3.4",
    "drupal/field_permissions": "~1.0-beta1",
    "drupal/field_validation": "^1.0@beta",
    "drupal/file_entity": "2.0-beta9",
    "drupal/focal_point": "^1.1",
    "drupal/gin": "^3.0@RC",
    "drupal/hide_revision_field": "^2.2",
    "drupal/inline_entity_form": "~1.0-beta1",
    "drupal/jquery_ui": "^1.6",
    "drupal/jquery_ui_tooltip": "^1.1",
    "drupal/jsonb": "~1.0-beta1",
    "drupal/migrate_manifest": "^3.0",
    "drupal/migrate_plus": "^6.0",
    "drupal/migrate_tools": "^6.0",
    "drupal/monolog": "dev-custom#b216caca850aa580ce7b21e618c5acfa2a24e344",
    "drupal/paragraphs": "^1.9",
    "drupal/redis": "^1.6",
    "drupal/restui": "1.x-dev",
    "drupal/scheduler": "^1.5",
    "drupal/services": "^4.0@beta",
    "drupal/transliterate_filenames": "^2.0",
    "drush/drush": "^10",
    "elife/api": "^2.24",
    "elife/api-sdk": "1.0.x-dev",
    "elife/api-validator": "^1.0@dev",
    "elife/bus-sdk": "dev-master",
    "embed/embed": "^3.0",
    "geerlingguy/drupal-vm": "^6.0",
    "giggsey/libphonenumber-for-php": "^7.7",
    "justinrainbow/json-schema": "^5.2",
    "league/commonmark": "^0.18.5",
    "league/html-to-markdown": "^4.7",
    "mindplay/composer-locator": "^2.1",
    "monolog/monolog": "~1.6",
    "paquettg/php-html-parser": "^2.0",
    "sebastian/version": "~2.0.1",
    "symfony/serializer": "4.4.47 as 3.4",
    "webmozart/path-util": "^2.3",
    "zenorocha/clipboardjs": "^1.7"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "conflict": {
    "drupal/drupal": "*"
  },
  "config": {
    "platform": {
      "php": "7.4"
    },
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "drupal/console-extend-plugin": true,
      "mindplay/composer-locator": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "drupal/core-composer-scaffold": true
    },
    "sort-packages": true
  },
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ]
  },
  "scripts": {
    "clean-up": "chmod -R 755 web/ && rm -rf config/local.settings.php web/ vendor/ private/",
    "post-install-cmd": [
      "JCMSDrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "post-update-cmd": [
      "JCMSDrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ]
  },
  "extra": {
    "composer-exit-on-patch-failure": true,
    "drupal-scaffold": {
      "locations": {
        "web-root": "web/"
      },
      "file-mapping": {
        "[web-root]/sites/default/default.services.yml": false,
        "[web-root]/sites/default/default.settings.php": false
      }
    },
    "installer-paths": {
      "web/core": [
        "type:drupal-core"
      ],
      "web/libraries/{$name}": [
        "type:drupal-library"
      ],
      "web/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "web/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "web/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ]
    },
    "patches": {
      "drupal/core": {
        "Move permission \"view any unpublished content\" from content moderation to core (273595)": "https://www.drupal.org/files/issues/2020-08-20/273595-166.patch",
        "Temporary fix for \\Drupal\\Core\\Field\\FieldItemList::generateSampleItems (2905527)": "src/patches/2905527-temporary-fix-generate-sample-items.patch"
      },
      "drupal/address": {
        "generateSampleValue (2819251)": "https://www.drupal.org/files/issues/2022-05-16/address-2819251-6.patch"
      },
      "embed/embed": {
        "Add twitter adapter to prevent errors on 429 status code": "https://github.com/denniszon/Embed/commit/56fc5273c02971ade656991dc9173b01753a07d9.patch"
      },
      "drupal/monolog": {
        "Prepare the module for D9 (3109707)": "src/patches/monolog-3109707-4.patch"
      },
      "drupal/duration_field": {
        "Field storage of field shared amongst several bundles is processed more than once when updating 1.x â†’ 2.x": "https://www.drupal.org/files/issues/2022-11-25/duration_field-ignore_field_update_with_proper_settings-3079754-7.patch"
      }
    }
  },
  "require-dev": {
    "drupal/core-dev": "^9.5",
    "drupal/drupal-extension": "^4.2"
  }
}
